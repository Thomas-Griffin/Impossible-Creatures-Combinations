  "description": "An application for Impossible Creatures that allows users to view all creature combinations.",
  "name": "combinations-web-app",
  "version": "1.0.0",
  "private": "true",
  "main": "dist/src/backend/start.js",
  "scripts": {
    "build": "nuxt build && tsc",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "backend:development": "cross-env ENVIRONMENT=development tsx src/backend/start.ts",
    "backend:production": "cross-env ENVIRONMENT=production tsx src/backend/start.ts",
    "backend:test": "cross-env ENVIRONMENT=test jest --runInBand --detectOpenHandles --forceExit",
    "backend:test:coverage": "cross-env ENVIRONMENT=test jest --runInBand --detectOpenHandles --forceExit --coverage ",
    "backend:docs": "tsx src/backend/utility/swagger.ts",
    "backend:compress-mods": "cross-env ENVIRONMENT=development tsx src/backend/database/compressMods.ts",
    "frontend:build": "nuxt build",
    "frontend:development": "nuxt dev",
    "frontend:generate": "nuxt generate",
    "frontend:preview": "nuxt preview",
    "frontend:postinstall": "nuxt prepare",
    "electron:development": "cross-env ENVIRONMENT=development electron .",
    "electron:build": "electron-builder"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "lodash": "^4.17.21",
    "mongodb": "^6.7.0",
    "msgpack-lite": "^0.1.26",
    "node-cache": "^5.1.2",
    "pinia": "^2.1.7",
    "pino": "^9.1.0",
    "pino-caller": "^3.4.0",
    "plotly.js-dist": "^2.33.0",
    "plotly.js-dist-min": "^2.33.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^3.52.0",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@pinia/nuxt": "^0.5.1",
    "@shelf/jest-mongodb": "^4.3.2",
    "@swc/core": "^1.5.24",
    "@swc/helpers": "^0.5.11",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.4",
    "@types/morgan": "^1.9.9",
    "@types/msgpack-lite": "^0.1.11",
    "@types/node": "^20.14.0",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "electron": "^30.0.9",
    "electron-builder": "^24.13.3",
    "jest": "^29.7.0",
    "mongodb": "^6.7.0",
    "morgan": "^1.10.0",
    "nuxt": "^3.11.2",
    "nuxt-plotly": "^1.0.13",
    "nuxt-primevue": "^3.0.0",
    "nuxt-storm": "^1.1.3",
    "pino-pretty": "^11.1.0",
    "prettier": "^3.3.0",
    "regenerator-runtime": "^0.14.1",
    "supertest": "^7.0.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.1.4",
    "tsx": "^4.11.2",
    "typescript": "^5.4.5",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "build": {
    "appId": "com.impossiblecreatures.combinations",
    "productName": "IC Combinations",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "electron/**/*",
      "package.json",
      "swagger.json"
    ],
    "win": {
      "icon": "src/frontend/public/icons/icon.png"
    },
    "mac": {
      "icon": "src/frontend/public/icons/icon.png"
    },
    "linux": {
      "icon": "src/frontend/public/icons/icon.png"
    }
  }
}
