{
  "arrowParens": "avoid",
  "bracketSpacing": false,
  "printWidth": 120,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "es5",
  "semi": false,
  "vue": {
    "bracketSpacing": false,
    "html": {
      "void": "always",
      "normal": "always",
      "component": "always",
      "closingBracket": false
    },
    "htmlWhitespaceSensitivity": "all",
    "semi": false,
    "singleAttributePerLine": true,
    "singleQuote": true
  }
}