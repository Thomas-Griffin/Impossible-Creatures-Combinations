{
  "name": "server",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "start.ts",
  "files": [
    "./swagger.json"
  ],
  "scripts": {
    "build": "tsc && cp -R ../backend/*.json dist/ && cp -R ../backend/*.msgpack.gz dist/",
    "server:development": "npm run build && cross-env environment=dev node dist/start.js",
    "server:production": "environment=production node dist/start.js",
    "test": "npm run build && cross-env environment=test jest --runInBand --detectOpenHandles --forceExit",
    "test:coverage": "npm run build && cross-env environment=test jest --coverage --runInBand --detectOpenHandles --forceExit",
    "docs": "npm run build && node dist/swagger.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "mongodb": "^6.3.0",
    "msgpack-lite": "^0.1.26",
    "node-cache": "^5.1.2",
    "axios": "^1.6.2",
    "plotly.js-dist-min": "^2.27.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/morgan": "^1.9.9",
    "@types/msgpack-lite": "^0.1.11",
    "@types/node": "^20.9.4",
    "@types/supertest": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@babel/preset-env": "7.23.5",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "prettier": "^3.1.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  }
}
