{
  "name": "combinations-backend",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "start.ts",
  "files": [
    "./swagger.json"
  ],
  "scripts": {
    "backend:build": "tsc && cp -R ./*.json dist/ && cp -R ./*.msgpack.gz dist/ && cp -R ./src/env dist/src/",
    "backend:development": "npm run backend:build && cross-env ENVIRONMENT=development node dist/start.js",
    "backend:production": "ENVIRONMENT=production node dist/start.js",
    "backend:test": "npm run backend:build && cross-env ENVIRONMENT=test jest --detectOpenHandles --forceExit",
    "backend:test:coverage": "npm run backend:build && cross-env ENVIRONMENT=test jest --coverage --runInBand --detectOpenHandles --forceExit",
    "backend:docs": "npm run backend:build && node dist/swagger.js",
    "backend:mod-compressor": "npm run backend:build && cross-env ENVIRONMENT=development node dist/database/compressor.js"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "mongodb": "^6.6.1",
    "msgpack-lite": "^0.1.26",
    "node-cache": "^5.1.2",
    "pino": "^9.0.0",
    "pino-caller": "^3.4.0",
    "plotly.js-dist-min": "^2.32.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@shelf/jest-mongodb": "^4.3.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/msgpack-lite": "^0.1.11",
    "@types/node": "^20.12.11",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "mongodb": "^6.6.1",
    "morgan": "^1.10.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  }
}
