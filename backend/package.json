{
  "name": "combinations-backend",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "start.ts",
  "files": [
    "./swagger.json"
  ],
  "scripts": {
    "backend:build": "tsc && cp -R ../backend/*.json dist/ && cp -R ../backend/*.msgpack.gz dist/",
    "backend:development": "npm run backend:build && cross-env ENVIRONMENT=development node dist/start.js",
    "backend:production": "ENVIRONMENT=production node dist/start.js",
    "backend:test": "npm run backend:build && cross-env ENVIRONMENT=test jest --runInBand --detectOpenHandles --forceExit",
    "backend:test:coverage": "npm run backend:build && cross-env ENVIRONMENT=test jest --coverage --runInBand --detectOpenHandles --forceExit",
    "backend:docs": "npm run backend:build && node dist/swagger.js"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.1",
    "mongodb": "^6.3.0",
    "msgpack-lite": "^0.1.26",
    "node-cache": "^5.1.2",
    "plotly.js-dist-min": "^2.28.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/msgpack-lite": "^0.1.11",
    "@types/node": "^20.11.2",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "prettier": "^3.2.2",
    "supertest": "^6.3.4",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
