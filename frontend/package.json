{
  "name": "combinations",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Combinations",
  "author": "Thomas Griffin <40271229+Thomas-Griffin@users.noreply.github.com>",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "frontend:development": "cross-env NODE_ENV=development quasar dev --mode pwa",
    "frontend:production": "cross-env NODE_ENV=production quasar build --mode pwa",
    "build": "quasar build --mode pwa --debug",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:8080 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:8080 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@cypress/code-coverage": "^3.12.11",
    "@quasar/extras": "^1.16.8",
    "axios": "^1.6.2",
    "cross-env": "^7.0.3",
    "joi": "^17.11.0",
    "pinia": "^2.1.7",
    "plotly.js-dist-min": "^2.27.1",
    "quasar": "^2.14.0",
    "vue": "^3.3.9",
    "vue-i18n": "^9.7.1",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@quasar/app-vite": "^1.7.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.1",
    "@types/node": "^20.10.0",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.18.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "workbox-build": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
